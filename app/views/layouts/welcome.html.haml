      #comments.container
        %h1.customheader.text-center Submit Comments
        .row
          %form{:role => "form"}
            %div
              .well.well-sm
                %strong
                  %span.glyphicon.glyphicon-asterisk>
                  Required Field
              .form-group
                %label{:for => "InputName"} Enter Name
                .input-group
                  %input#InputName.form-control{:name => "InputName", :placeholder => "Enter Name", :required => "", :type => "text"}
                    %span.input-group-addon
                      %span.glyphicon.glyphicon-asterisk
              .form-group
                %label{:for => "InputEmail"} Enter Email
                .input-group
                  %input#InputEmailFirst.form-control{:name => "InputEmail", :placeholder => "Enter Email", :required => "", :type => "email"}
                    %span.input-group-addon
                      %span.glyphicon.glyphicon-asterisk
              .form-group
                %label{:for => "InputEmail"} Confirm Email
                .input-group
                  %input#InputEmailSecond.form-control{:name => "InputEmail", :placeholder => "Confirm Email", :required => "", :type => "email"}
                    %span.input-group-addon
                      %span.glyphicon.glyphicon-asterisk
              .form-group
                %label{:for => "InputMessage"} Enter Message
                .input-group
                  %textarea#InputMessage.form-control{:name => "InputMessage", :required => "", :rows => "5"}
                  %span.input-group-addon
                    %span.glyphicon.glyphicon-asterisk
              %input#submit.btn.btn-info.pull-right{:name => "submit", :type => "submit", :value => "Submit"}
