          .col-md-4
            .panel.panel-default
              .panel-heading
                %h4.text-center Personal Package
              .panel-body.text-center
                %p.lead.pull-right
                  %strong $10 / month
              %ul.list-group.list-group-flush.text-center
                %li.list-group-item
                  Personal Use
                  %span.glyphicon.glyphicon-ok.pull-right
                %li.list-group-item
                  Single Commercial License
                  %span.glyphicon.glyphicon-remove.pull-right
                %li.list-group-item
                  Multiple site Commercial license
                  %span.glyphicon.glyphicon-remove.pull-right
                %li.list-group-item
                  Technical Support
                  %span.glyphicon.glyphicon-remove.pull-right
              .panel-footer
                %a.btn.btn-lg.btn-block.btn-default Purchase
          .col-md-4
            .panel.panel-info
              .panel-heading
                %h4.text-center Standard commercial package
              .panel-body.text-center
                %p.lead.pull-right
                  %strong $27 / month
              %ul.list-group.list-group-flush.text-center
                %li.list-group-item
                  Personal Use
                  %span.glyphicon.glyphicon-ok.pull-right
